{
  "services": [
    {
      "type": "web",
      "name": "wakili-pro-backend",
      "env": "node",
      "region": "oregon",
      "plan": "free",
      "rootDir": "backend",
      "buildCommand": "npm install && npm run build",
      "startCommand": "npm start",
      "healthCheckPath": "/api/health",
      "envVars": [
        {
          "key": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ]
}
