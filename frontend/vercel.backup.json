{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm ci",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://wakili-pro.onrender.com/api",
    "VITE_APP_NAME": "Wakili Pro",
    "VITE_APP_VERSION": "1.0.0"
  }
}