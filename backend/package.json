{
  "name": "@wakili-pro/backend",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Wakili Pro Backend - Minimal Version",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "cd ../shared && npm run build",
    "build": "tsc --skipLibCheck",
    "postbuild": "node -e \"console.log('Build complete. Dist folder:', require('fs').existsSync('./dist') ? 'EXISTS' : 'MISSING'); if (require('fs').existsSync('./dist/index.js')) console.log('index.js found!'); else console.error('ERROR: index.js not found!');\"",
    "start": "node --max-old-space-size=4096 start.js",
    "start:direct": "node --max-old-space-size=4096 dist/index.js",
    "dev": "node --max-old-space-size=4096 dist/index.js",
    "lint": "echo 'Backend linting skipped - no ESLint config present. Will add proper linting later.'",
    "test": "node --max-old-space-size=4096 ./node_modules/.bin/jest",
    "postinstall": "prisma generate",
    "seed:users": "ts-node ./prisma/seedUsers.ts",
    "seed:lawyers": "ts-node ./prisma/seedLawyerSubscriptions.ts",
    "seed:docs": "ts-node ./prisma/seedDocumentTemplates.ts",
    "seed:all": "npm run seed:users && npm run seed:lawyers && npm run seed:docs",
    "seed:auto": "ts-node ./prisma/seedAll.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.930.0",
    "@aws-sdk/lib-storage": "^3.930.0",
    "@aws-sdk/s3-request-presigner": "^3.930.0",
    "@prisma/client": "^5.22.0",
    "@types/compression": "^1.8.1",
    "@types/express-rate-limit": "^6.0.2",
    "@types/multer": "^2.0.0",
    "@types/socket.io": "^3.0.2",
    "bcryptjs": "^3.0.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "openai": "^6.8.1",
    "rate-limit-redis": "^4.2.3",
    "react-hot-toast": "^2.6.0",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.1",
    "uuid": "^13.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@types/bcryptjs": "^3.0.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/stripe": "^8.0.417",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prisma": "^5.12.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
